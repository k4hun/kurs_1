.hero-unit
  %h1 New category

  = form_for(@category, :url => { :action => "create" }) do |f| 
    = render(:partial => 'form', :locals => {:f => f})
    %p{:style => 'text-align: center;'}
      =f.submit(:Create, :class => 'btn btn-primary')
      = link_to("Back", { :action => "index" }, :class => 'btn btn-warning')